# OriginFD Web Application Environment Variables
# Copy this file to .env.local and customize the values

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here-change-in-production

# API URLs
API_BASE_URL=http://localhost:8000
# NEXT_PUBLIC_API_URL=http://localhost:8000 # Uncomment to proxy to external API
NEXT_PUBLIC_ORCHESTRATOR_URL=http://localhost:8001

# Development flags
NEXT_PUBLIC_ENV=development
NEXT_PUBLIC_LIFECYCLE_V2=1
AGENTS_ENABLED=0

# Database (used by API prestart scripts)
DATABASE_URL=sqlite:///./dev.db

# Telemetry settings (set to 1 to disable)
NEXT_TELEMETRY_DISABLED=1
TURBO_TELEMETRY_DISABLED=1
